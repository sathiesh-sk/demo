public class Tables {
    public static void main(String[] args) {
        for (int num = 2; num <= 3; num++) {
            System.out.println("Table of " + num + ":");
            for (int i = 1; i <= 10; i++) {
                System.out.println(num + " x " + i + " = " + (num * i));
            }
            System.out.println(); // blank line between tables
        }
    }
}
