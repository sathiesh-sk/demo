import java.util.Scanner;
class For{
    public static void main(String args[])
    {
        Scanner sk = new Scanner(System.in);
        int a = sk.nextInt();
        for(int i=1;i<=a;i++){
            for(int j=i;j>0;j--){
            System.out.print("*");
            }
            System.out.println();
        }
    }
}        
        
