class leapyear{
    public static void main(String args[])
    {
        int a = 2000;
        if(a%4==0)
        {
           System.out.println("Leap Year");
           if(a%400==0)
           {
               System.out.print("It is century");
           }
           else{
                 System.out.print("It is Non-century");
           }
        }
        else
        {
           System.out.println("Non-Leap Year"); 
           if(a%400==0)
           {
               System.out.print("It is century");
           }
           else{
                 System.out.print("It is Non-century");
           }
        }
    }
}

