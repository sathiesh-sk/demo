import java.util.Scanner;
class evenforif{
    public static void main(String args[])
    {
        Scanner sk = new Scanner(System.in);
        int a = sk.nextInt();

        int evencount=0;
        int oddcount=0;
        for(int i=a;i<=10;i++)
        {
            if(i%2==0)
            {
                System.out.println("Even Number :"+i);
                evencount++;
            }
            else
            {
                System.out.println("Odd Number :"+i);
                oddcount++;
            }
        }
        System.out.println("The Count of Even no is :"+evencount);
        System.out.println("The Count of Odd no is :"+oddcount);
       
    }
}

